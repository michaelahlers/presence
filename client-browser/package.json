{
  "name": "client-browser",
  "version": "0.0.0",
  "description": "Personal web presence for Michael Ahlers.",
  "scripts": {
    "clean": "rimraf build",
    "develop": "webpack-dev-server --progress --color",
    "build": "webpack --progress --color",
    "deploy": "aws s3 sync --dryrun --delete --exclude 'settings.json' 'build/'",
    "deploy-unsafe": "aws s3 sync --delete --exclude 'settings.json' 'build/'",
    "test": "jest --verbose"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "jest": {
    "testRunner": "<rootDir>/node_modules/jest-cli/src/testRunners/jasmine/jasmine2",
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/michaelahlers/michaelahlers-presence.git"
  },
  "author": "Michael Ahlers <michael@ahlers.co>",
  "license": "MIT",
  "bugs": {
    "url": "http://github.com/michaelahlers/michaelahlers-presence/issues"
  },
  "homepage": "http://github.com/michaelahlers/michaelahlers-presence/wiki",
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "babel-runtime": "^6.3.19",
    "color": "^0.11.1",
    "font-awesome": "^4.5.0",
    "immutable": "^3.7.6",
    "radium": "^0.16.2",
    "react": "^0.14.6",
    "react-router": "^2.0.0-rc5",
    "whatwg-fetch": "^0.11.0"
  },
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-eslint": "^5.0.0-beta6",
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-react": "^3.15.0",
    "html-webpack-plugin": "^2.7.1",
    "jasmine": "^2.4.1",
    "jest-cli": "^0.8.2",
    "minimist": "^1.2.0",
    "path": "^0.12.7",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.5.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1"
  }
}
